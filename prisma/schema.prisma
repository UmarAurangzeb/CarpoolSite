// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Carowner {
  nuid           String @id
  OwnerName      String
  carname        String
  monthlycharges Int    @default(0)
  completeRoute  String
  WhatsApp       String
}
